import java.util.Random;

public class Ex00 {

	public static void main(String[] args) {
		
		// 랜덤으로 배열을 만드는데 겹치는 요소가 없도록 만들어 보자!!!
		
		Random random = new Random();
		
		int i = 0;
		int j = 0;
		int[] nums = new int[10];
		
		int sameFactor = 1;
		
		
		for (i = 0; i < nums.length; i++ ) {
			nums[i] = random.nextInt(10);
			
			for (j = 0; j < i; j++) {
				if (nums[i] == nums[j]) {
					sameFactor *= 0;
				}
			}
			
			
			
			
			System.out.print(nums[i] + " ");
			
			
			
		}
	}
}
